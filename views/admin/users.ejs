        <div class="col-12 grid-margin">
          <div class="card">
            <div class="card-body">
              <h4 class="card-title">All Rents</h4>
              <div class="table-responsive">
                <table class="table">
                  <thead>
                    <tr>
                      
                      <th> Client Name </th>
                      <th> Email </th>
                    </tr>
                  </thead>
                  <tbody>
                  <% users.forEach(user  => { %>
                       
                    <tr>
                     
